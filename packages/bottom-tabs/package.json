    "react-native-component",
    "react-component",
    "react-native",
    "react-navigation",
    "ios",
    "android",
    "tab"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/react-navigation/react-navigation.git",
    "directory": "packages/bottom-tabs"
  },
  "main": "lib/commonjs/index.js",
